<?xml version="1.0"?>
<package format="2">
  <name>pal_ros_utils</name>
  <version>0.3.3</version>
  <description>The pal_ros_utils package</description>

  <maintainer email="victor.lopez@pal-robotics.com">Victor Lopez</maintainer>
  <maintainer email="hilario.tome@pal-robotics.com">Hilario Tome</maintainer>

  <license>Proprietary</license>
  <buildtool_depend>catkin</buildtool_depend>

  <depend>roscpp</depend>
  <depend>rospy</depend>
  <depend>ros_comm</depend>
  <depend>geometry_msgs</depend>
  <depend>eigen</depend>
  <depend>tf</depend>
  <depend>cmake_modules</depend>
  <depend>math_utils</depend>
  <depend>pal_geometry_tools</depend>
  <depend>pal_utils</depend>
  <depend>resource_retriever</depend>
  <depend>geometric_shapes</depend>
  <depend>property_bag</depend>
  <depend>ddynamic_reconfigure</depend>
  <depend>dynamic_introspection</depend>
  <depend>interactive_markers</depend>
  <depend>urdf</depend>
  <depend>pluginlib</depend>
  <depend>pal_ariles</depend>
  <depend>pal_joint_trajectory_controller</depend>
  <depend>pal_control_msgs</depend>
  <depend>pal_statistics</depend>
  <depend>sensor_msgs</depend>
  <depend>mm11_msgs</depend>

  <test_depend>rostest</test_depend>
  <test_depend>eigen_checks</test_depend>
  <test_depend>pal_test_utils</test_depend>

  <export>
     <pal_ros_utils plugin="${prefix}/reference_plugins.xml" />
  </export>
</package>
