<?xml version="1.0"?>
<package format="2">
  <name>pal_ros_control</name>
  <version>2.4.3</version>
  <description>Layer between ROS control and RTT</description>

  <maintainer email="adolfo.rodriguez@pal-robotics.com">Adolfo Rodriguez Tsouroukdissian</maintainer>
  <author email="adolfo.rodriguez@pal-robotics.com">Adolfo Rodriguez Tsouroukdissian</author>

  <license>LGPL-3.0</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rtt_ros</build_depend>

  <depend>controller_manager</depend>
  <depend>hardware_interface</depend>
  <depend>joint_limits_interface</depend>
  <depend>realtime_tools</depend>
  <depend>roscpp</depend>
  <depend>rtt</depend>
  <depend>rtt_std_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>std_srvs</depend>
  <depend>transmission_interface</depend>
  <depend>urdf</depend>
  <depend>pal_hardware_interfaces</depend>
  <depend>math_utils</depend>
  <depend>pal_utils</depend>
  <depend>pal_controller_interface</depend>
  <depend>dynamic_introspection</depend>

  <test_depend>dummy_actuators_manager</test_depend>
  <test_depend>rostest</test_depend>
  <test_depend>xacro</test_depend>

  <export>
    <rtt_ros><plugin_depend>pal_ros_control</plugin_depend></rtt_ros>
  </export>

</package>
