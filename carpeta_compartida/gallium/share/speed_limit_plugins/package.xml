<?xml version="1.0"?>
<package format="2">
  <name>speed_limit_plugins</name>
  <version>2.0.5</version>
  <description>
    The speed_limit_plugins package
    contains the plugins for speed_limit.
  </description>

  <author email="jeremie.deray@pal-robotics.com">Jeremie Deray</author>
  <maintainer email="jeremie.deray@pal-robotics.com">Jeremie Deray</maintainer>

  <license>Proprietary</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>geometry_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>speed_limit_core</depend>
  <depend>tf2_sensor_msgs</depend>

<!--  <test_depend>roslib</test_depend>-->
<!--  <test_depend>rostest</test_depend>-->

  <export>
    <speed_limit_core plugin="${prefix}/speed_limit_plugins.xml" />
  </export>
</package>
