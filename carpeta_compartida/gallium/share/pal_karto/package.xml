<package format="2">
  <name>pal_karto</name>
  <description>
     ROS wrapper for OpenKarto.
  </description>
  <version>1.3.6</version>
  <maintainer email="jon.binney@gmail.com">Jon Binney</maintainer>
  <author>Brian Gerkey</author>
  <license>LGPL</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>cmake_modules</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>angles</build_depend>
  <build_depend>dynamic_reconfigure</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>visualization_msgs</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>rosconsole</build_depend>
  <build_depend>nav_msgs</build_depend>
  <build_depend>message_filters</build_depend>
  <build_depend>sba</build_depend>
  <build_depend>open_karto</build_depend>
  <build_depend>pal_map_utils</build_depend>
  <build_depend>interactive_markers</build_depend>
  <build_depend>graph_msgs</build_depend>
  <build_depend>pluginlib</build_depend>
  <build_depend>pal_interaction_msgs</build_depend>
  <build_depend>rosbag</build_depend>
  <build_depend>math_utils</build_depend>
  <build_depend>message_generation</build_depend>

  <exec_depend>cmake_modules</exec_depend>
  <exec_depend>roscpp</exec_depend>
  <exec_depend>angles</exec_depend>
  <exec_depend>dynamic_reconfigure</exec_depend>
  <exec_depend>tf</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>visualization_msgs</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>
  <exec_depend>rosconsole</exec_depend>
  <exec_depend>nav_msgs</exec_depend>
  <exec_depend>message_filters</exec_depend>
  <exec_depend>sba</exec_depend>
  <exec_depend>open_karto</exec_depend>
  <exec_depend>pal_map_utils</exec_depend>
  <exec_depend>interactive_markers</exec_depend>
  <exec_depend>graph_msgs</exec_depend>
  <exec_depend>pluginlib</exec_depend>
  <exec_depend>pal_interaction_msgs</exec_depend>
  <exec_depend>rosbag</exec_depend>
  <exec_depend>math_utils</exec_depend>
  <exec_depend>message_runtime</exec_depend>

  <!-- Tests -->
  <build_depend>rostest</build_depend>
  <test_depend>rosnode</test_depend>

  <export>
    <pal_karto plugin="${prefix}/pal_karto_solver_plugins.xml" />
  </export>
</package>


