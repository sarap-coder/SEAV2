<?xml version="1.0"?>
<package format="2">
  <name>pal_waypoint</name>
  <version>0.4.4</version>
  <description>A simple tool to set and follow waypoints</description>

  <maintainer email="victor.lopez@pal-robotics.com">Victor Lopez</maintainer>

  <author email="paul.mathieu@pal-robotics.com">Paul Mathieu</author>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>roscpp</depend>
  <depend>actionlib</depend>
  <depend>geometry_msgs</depend>
  <depend>nav_msgs</depend>
  <depend>actionlib_msgs</depend>
  <depend>interactive_markers</depend>
  <depend>visualization_msgs</depend>
  <depend>pal_navigation_msgs</depend>
  <depend>pal_composite_navigation_msgs</depend>
  <depend>pal_map_utils</depend>
  <depend>corridor_exploration_msgs</depend>
  <depend>tf</depend>
  <depend>pal_waypoint_msgs</depend>
  <depend>diagnostic_updater</depend>

  <exec_depend>rospy</exec_depend>
  <exec_depend>pal_navigation_manager</exec_depend>

  <test_depend>gmock</test_depend>
  <test_depend>rostest</test_depend>
</package>
