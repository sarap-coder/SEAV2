<?xml version="1.0" encoding="UTF-8"?>
<launch>

 <arg name="joint_name" default=""/>
 <arg name="controller_pkg_name" default=""/>
 <arg name="ns" default=""/>
 <arg name="check_mode" default="false"/>
 <arg name="open_loop" default="false"/>
 <arg name="safety_override" default=""/>

 <rosparam command="load" ns="$(eval arg('ns') + '/joints/' + arg('joint_name') + '/actuator_params')" file="$(eval find(arg('controller_pkg_name')) + '/config/local_joint_control/actuator_parameters/' + arg('joint_name') + '.yaml')" />
 <param name="$(eval arg('ns') + '/joints/' + arg('joint_name') + '/type')" value="pal_local_joint_control::DirectPositionControl"/> 
 <param name="$(eval arg('ns') + '/joints/' + arg('joint_name') + '/check_mode')" value="$(arg check_mode)"/> 
 <param name="$(eval arg('ns') + '/joints/' + arg('joint_name') + '/open_loop')" value="$(arg open_loop)"/> 
</launch>
