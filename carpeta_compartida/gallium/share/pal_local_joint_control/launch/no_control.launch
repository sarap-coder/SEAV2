<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <arg name="joint_name" default=""/>
  <arg name="controller_pkg_name" default=""/>
  <arg name="ns" default=""/>
  <arg name="open_loop" default="false"/>
  <arg name="safety_override" default=""/>

  <rosparam
    command="load"
    ns="$(eval arg('ns') + '/joints/' + arg('joint_name') + '/controller_params')"
    file="$(eval find(arg('controller_pkg_name')) + '/config/local_joint_control/no_control/' + arg('joint_name') + '_params.yaml')" />

  <param name="$(eval arg('ns') + '/joints/' + arg('joint_name') + '/type')" value="pal_local_joint_control::NoControl"/>

</launch>
