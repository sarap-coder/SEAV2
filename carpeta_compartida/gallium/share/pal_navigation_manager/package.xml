<?xml version="1.0"?>
<package format="2">
  <name>pal_navigation_manager</name>
  <version>0.1.13</version>
  <description>
    Manager on top of the navigation stack.
    It allows to configures move_base correctly and manage the goal feedback.
  </description>

  <author email="enrique.fernandez@pal-robotics.com">Enrique Fernandez</author>
  <author email="jose.garcia@pal-robotics.com">Jose Carlos Garcia</author>

  <maintainer email="jose.garcia@pal-robotics.com">Jose Carlos Garcia</maintainer>

  <license>Proprietary</license>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>actionlib</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>roslib</build_depend>
  <build_depend>pal_navigation_msgs</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>move_base_msgs</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>tf2</build_depend>
  <build_depend>tf2_ros</build_depend>
  <build_depend>angles</build_depend>

  <exec_depend>actionlib</exec_depend>
  <exec_depend>roscpp</exec_depend>
  <exec_depend>roslib</exec_depend>
  <exec_depend>pal_navigation_msgs</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>
  <exec_depend>move_base_msgs</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>tf2</exec_depend>
  <exec_depend>tf2_ros</exec_depend>
  <exec_depend>angles</exec_depend>

  <test_depend>pal_test_utils</test_depend>
</package>
